{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start Frontend",
            "type": "shell",
            "command": "npm",
            "args": ["start"],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Build Frontend",
            "type": "shell",
            "command": "npm",
            "args": ["run", "build"],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        },
        {
            "label": "Test Frontend",
            "type": "shell",
            "command": "npm",
            "args": ["test"],
            "group": {
                "kind": "test",
                "isDefault": false
            }
        },
        {
            "label": "Eject Frontend",
            "type": "shell",
            "command": "npm",
            "args": ["run", "eject"],
            "group": {
                "kind": "build",
                "isDefault": false
            }
        }
    ]
}
